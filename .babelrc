{
    "presets": [
        [
            "@babel/preset-env"
        ],
        [
            "@babel/preset-react"
        ]
    ],
    "plugins": [
        ["import", {"libraryName": "antd", "libraryDirectory": "lib", "style": "css"}],
        ["@babel/plugin-transform-runtime",{"corejs": 2}],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "react-hot-loader/babel"
    ]
}